    "\n",
    "print('CALCULATOR')\n",
    "a=float(input('enter a number:'))\n",
    "b=float(input('enter another number:'))\n",
    "c=input('enter operation + * - / :')\n",
    "\n",
    "if c=='+':\n",
    "  d=a+b\n",
    "elif c=='-':\n",
    "  d=a-b\n",
    "elif c=='*':\n",
    "  d=a*b\n",
    "elif c=='/':\n",
    "  d=a/b\n",
    "elif c=='^':\n",
    "    d=a**b\n",
    "else:\n",
    "  d='Enter Valid Operator'\n",
    "\n",
    "print(d)\n"
